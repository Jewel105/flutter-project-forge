{
  "name": "flutter-project-forge",
  "displayName": "Flutter Project Forge",
  "description": "Automate code generation in a Flutter project.",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jewel105/flutter-project-forge.git"
  },
  "publisher": "JewelYi",
  "icon": "logo.png",
  "README": "README.md",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "flutter",
    "dart",
    "template",
    "demo",
    "generator"
  ],
  "activationEvents": [
    "workspaceContains:**/pubspec.yaml"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutter-project-forge.createFlutterDemo",
        "category": "FlutterProjectForge",
        "title": "Forge Demo"
      },
      {
        "command": "flutter-project-forge.http",
        "category": "FlutterProjectForge",
        "title": "Forge HTTP"
      },
      {
        "command": "flutter-project-forge.sqlite",
        "category": "FlutterProjectForge",
        "title": "Forge Database"
      },
      {
        "command": "flutter-project-forge.createPage",
        "category": "FlutterProjectForge",
        "title": "Forge Page"
      },
      {
        "command": "flutter-project-forge.scan",
        "category": "FlutterProjectForge",
        "title": "Forge Scan"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "flutter-project-forge.createFlutterDemo",
          "group": "Forge@1"
        },
        {
          "command": "flutter-project-forge.createPage",
          "group": "Forge@1"
        },
        {
          "command": "flutter-project-forge.http",
          "group": "Forge@2"
        },
        {
          "command": "flutter-project-forge.sqlite",
          "group": "Forge@3"
        },
        {
          "command": "flutter-project-forge.scan",
          "group": "Forge@4"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "publish": "vsce publish patch",
    "prepare": "husky",
    "changelog": "conventional-changelog -p -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "conventional-changelog-cli": "^5.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3"
  }
}
